          </item>
          <item>
            <attribute name="label">Mac (_CR)</attribute>
            <attribute name="action">document.line-ending</attribute>
            <attribute name="target" type="i">1</attribute>
          </item>
          <item>
            <attribute name="label">DOS / Windows (C_R LF)</attribute>
