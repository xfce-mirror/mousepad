            <item>
              <attribute name="label">3</attribute>
              <attribute name="action">document.tab.tab-size</attribute>
              <attribute name="target" type="i">3</attribute>
            </item>
            <item>
              <attribute name="label">4</attribute>
              <attribute name="action">document.tab.tab-size</attribute>
              <attribute name="target" type="i">4</attribute>
